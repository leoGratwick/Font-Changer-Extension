{
    "manifest_version": 3,
    "name": "Font Changer",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "action": {
        "default_popup": "Popup/index.html",
        "default_title": "Font Changer",
        "default_icon": {
            "16": "images/icon16.png",   
            "48": "images/icon48.png",   
            "128": "images/icon128.png" 
          }

        

    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}