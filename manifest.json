{
    "manifest_version": 3,
    "name": "Font Changer",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "action": {
        "default_popup": "Popup/index.html",
        "default_title": "Font Changer"

    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ]
}